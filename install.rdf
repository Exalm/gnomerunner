<?xml version="1.0"?>
<RDF:RDF xmlns:em="http://www.mozilla.org/2004/em-rdf#"
     xmlns:NC="http://home.netscape.com/NC-rdf#"
     xmlns:RDF="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <Description about="urn:mozilla:install-manifest">
        <em:id>$THEME_ID</em:id>
        <em:internalName>$INTERNAL_NAME</em:internalName>
        <em:skinnable>true</em:skinnable>
        <em:type>32</em:type>
        <em:name>$NAME</em:name>
        <em:description>$THEME_DESC</em:description>
        <em:version>$VERSION</em:version>
        <em:creator>Exalm</em:creator>
        <em:targetApplication>
            <Description>
                <em:id>$SM_ID</em:id>
                <em:minVersion>$SM_MIN_VERSION</em:minVersion>
                <em:maxVersion>$SM_MAX_VERSION</em:maxVersion>
            </Description>
        </em:targetApplication>
    </Description>
</RDF:RDF>
