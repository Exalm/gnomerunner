<?xml version="1.0"?>
<RDF xmlns:em="http://www.mozilla.org/2004/em-rdf#"
     xmlns:NC="http://home.netscape.com/NC-rdf#"
     xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <Description about="urn:mozilla:install-manifest">
        <em:id>$OPTIONS_ID</em:id>
        <em:type>2</em:type>

        <em:name>GNOMErunner Options</em:name>
        <em:description>Allows to customize GNOMErunner theme.</em:description>
        <em:version>$VERSION</em:version>
        <em:optionsURL>chrome://gnomerunner-options/content/options.xul</em:optionsURL>

        <em:creator>Exalm</em:creator>

        <em:targetApplication>
            <Description>
                <em:id>$SM_ID</em:id>
                <em:minVersion>$SM_MIN_VERSION</em:minVersion>
                <em:maxVersion>$SM_MAX_VERSION</em:maxVersion>
            </Description>
        </em:targetApplication>
    </Description>
</RDF>
